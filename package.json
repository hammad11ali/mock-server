{
  "name": "express-mock-server",
  "version": "1.0.0",
  "description": "A mock server using Express and TypeScript to provide mock JSON responses.",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"src/config/**/*\" dist && copyfiles -u 1 \"src/data/**/*\" dist && copyfiles src/admin.html dist",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/marked": "^5.0.2",
    "express": "^4.17.1",
    "highlight.js": "^11.11.1",
    "marked": "^16.4.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.13",
    "@types/node": "^16.11.7",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "license": "MIT"
}
